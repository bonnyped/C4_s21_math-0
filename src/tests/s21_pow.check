#suite s21_pow_tests

#test s21_pow_1
  double base = 0.0;
  double exp = 0.0;
  long double orig = pow(base, exp);
  long double s21 = s21_pow(base, exp);
  ck_assert_ldouble_eq(orig, s21);

#test s21_pow_2
  double base = 4.0;
  double exp = 1.011;
  long double orig = pow(base, exp);
  long double s21 = s21_pow(base, exp);
  ck_assert_ldouble_eq_tol(orig, s21, 10e-7);