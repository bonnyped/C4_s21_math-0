#suite s21_log_tests

#test s21_log_1
  double a = 0;
  long double orig = log(a);
  long double s21 = s21_log(a);
  ck_assert_ldouble_eq(orig, s21);

#test s21_log_1_1
  double a = 0.0;
  long double orig = log(a);
  long double s21 = s21_log(a);
  ck_assert_ldouble_eq(orig, s21);

#test s21_log_1_2
  double a = -0.0;
  long double orig = log(a);
  long double s21 = s21_log(a);
  ck_assert_ldouble_eq(orig, s21);

#test s21_log_1_3
  double a = -0;
  long double orig = log(a);
  long double s21 = s21_log(a);
  ck_assert_ldouble_eq(orig, s21);

#test s21_log_1_4
  double a = -0.0 / 0.0;
  long double orig = log(a);
  long double s21 = s21_log(a);
  ck_assert_ldouble_nan(orig);
  ck_assert_ldouble_nan(s21);

#test s21_log_1_5
  double a = 0.0 / 0.0;
