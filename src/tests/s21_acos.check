#suite s21_acos_tests

#test s21_acos_1
  double a = 0.0;
  long double orig = acos(a);
  long double s21 = s21_acos(a);
  ck_assert_ldouble_eq_tol(orig, s21, 10e-6);

#test s21_acos_1_2
  double a = -0.0;
  long double orig = acos(a);
  long double s21 = s21_acos(a);
  ck_assert_ldouble_eq_tol(orig, s21, 10e-7);

