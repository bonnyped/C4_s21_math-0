#suite s21_cos_tests

#test s21_cos_2
  double a = 0.01;
  long double orig = cos(a);
  long double s21 = s21_cos(a);
  ck_assert_ldouble_eq_tol(orig, s21, 10e-7);

#test s21_cos_3
  double a = 123123123.123;
  long double orig = cos(a);
  long double s21 = s21_cos(a);
  ck_assert_ldouble_eq_tol(orig, s21, 10e-7);

#test s21_cos_4
  double a = -123123123.123;
  long double orig = cos(a);
  long double s21 = s21_cos(a);
  ck_assert_ldouble_eq_tol(orig, s21, 10e-7);

#test s21_cos_5
  double a = -0.453123;
  long double orig = cos(a);
  long double s21 = s21_cos(a);
  ck_assert_ldouble_eq_tol(orig, s21, 10e-7);

#test s21_cos_6
  double a = -0.0 / 0.0;
  long double orig = cos(a);
  long double s21 = s21_cos(a);
  ck_assert_ldouble_nan(s21);
  ck_assert_ldouble_nan(orig);

#test s21_cos_7
  double a = -1.0 / 0.0;
  long double orig = cos(a);
  long double s21 = s21_cos(a);
  ck_assert_ldouble_nan(s21);
  ck_assert_ldouble_nan(orig);

#test s21_cos_8
  double a = 1.0 / 0.0;
  long double orig = cos(a);
